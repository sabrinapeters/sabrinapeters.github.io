{
  "name": "sabrina-peters",
  "private": true,
  "version": "0.1.0",
  "author": "Sabrina Peters <brideeray@gmail.com>",
  "description": "Jekyll Setup for Sabrina's blog",
  "scripts": {
    "start": "jekyll serve",
    "css": "postcss _assets/stylesheets/main.css -o _site/assets/dist/bundle.css",
    "images": "imagemin _assets/images/**/* -o _site/assets/dists/img"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sabrinapeters/sabrinapeters.github.io.git"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "css-mqpacker": "^5.0.1",
    "cssnano": "^3.7.3",
    "del": "^2.2.1",
    "imagemin-cli": "^3.0.0",
    "postcss-apply": "^0.8.0",
    "postcss-cli": "^4.1.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^8.0.2",
    "postcss-reporter": "^5.0.0",
    "postcss-uncss": "^0.14.0",
    "prettier": "^1.10.2",
    "stylelint": "^7.2.0",
    "stylelint-config-wilco": "^1.0.2",
    "uncss": "^0.14.1"
  },
  "bugs": {
    "url": "https://github.com/sabrinapeters/sabrinapeters.github.io/issues"
  },
  "browserslist": [
    "last 2 version"
  ],
  "postcss": {
    "plugins": {
      "postcss-import": {},
      "postcss-cssnext": {
        "warnForDuplicates": true,
        "features": {
          "colorFunction": true,
          "customSelector": true,
          "rem": false
        }
      },
      "postcss-apply": {},
      "postcss-uncss": {
        "html": [
          "./_site/**/*.html"
        ],
        "ignore": [
          ".c-entry",
          ".c-entry *"
        ]
      },
      "css-mqpacker": {},
      "cssnano": {
        "autoprefixer": false,
        "mergeRules": false,
        "filterPlugins": false,
        "discardComments": {
          "removeAll": true
        }
      }
    }
  },
  "stylelint": {
    "extends": "stylelint-config-wilco"
  },
  "dependencies": {
    "obsidian.css": "^2.1.3"
  },
  "license": "MIT"
}
