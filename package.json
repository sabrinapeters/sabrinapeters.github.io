{
  "name": "sabrina-peters",
  "private": true,
  "version": "0.1.0",
  "author": "Sabrina Peters <brideeray@gmail.com>",
  "description": "Jekyll Setup for Sabrina's blog",
  "scripts": {
    "start": "bundle exec jekyll serve --incremental",
    "css": "postcss _assets/stylesheets/index.css -o _includes/bundle.css",
    "images": "imagemin _assets/images/**/* -o _site/assets/dists/img"
  },
  "repository": "sabrinapeters/sabrinapeters.github.io",
  "dependencies": {
    "obsidian.css": "^2.1.3"
  },
  "devDependencies": {
    "autoprefixer": "^9.3.1",
    "cssnano": "^4.1.7",
    "del": "^3.0.0",
    "imagemin-cli": "^3.0.0",
    "postcss-cli": "^6.0.1",
    "prettier": "^1.15.2"
  },
  "browserslist": [
    "last 1 version"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "cssnano": {
        "autoprefixer": false,
        "mergeRules": false,
        "filterPlugins": false,
        "discardComments": {
          "removeAll": true
        }
      }
    }
  }
}
